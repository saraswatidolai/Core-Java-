package com.r.lambda_expression;

interface Calculator{
	double getSquareAndCube(int num);
}
public class LambdaDemo5 {

	public static void main(String[] args) {
	
		Calculator square = num -> num*num;
		System.out.println("Square: "+square.getSquareAndCube(5));
		
		Calculator cube = num -> num*num*num;
		System.out.println("Square: "+cube.getSquareAndCube(5));
	}
}
